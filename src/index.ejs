<% var item, key %><%
htmlWebpackPlugin.options.lang = htmlWebpackPlugin.options.lang || "en" %><%
htmlWebpackPlugin.options.left_links = htmlWebpackPlugin.options.left_links || [] %><%
htmlWebpackPlugin.options.meta = htmlWebpackPlugin.options.meta || [] %><!DOCTYPE html>
<html lang="<%= htmlWebpackPlugin.options.lang %>">
<head>
    <title>Sitenav</title>
    <!-- HTML5 Shim and Respond.js IE11 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 11]>
		<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
		<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
		<![endif]-->
    <!-- Meta -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="" />
    <meta name="keywords" content=""/>
    <meta name="author" content="dubuqingfeng"/>
    <!-- Favicon icon -->
    <link rel="icon" href="assets/images/favicon.ico" type="image/x-icon">
</head>

<body>
    <!-- [ Pre-loader ] start -->
    <div class="loader-bg">
        <div class="loader-track">
            <div class="loader-fill"></div>
        </div>
    </div>
    <!-- [ Pre-loader ] End -->
    <!-- [ navigation menu ] start -->
    <nav class="pcoded-navbar">
        <div class="navbar-wrapper">
            <div class="navbar-brand header-logo">
                <a href="index.html" class="b-brand">
                    <span class="b-title">网址导航</span>
                </a>
                <a class="mobile-menu" id="mobile-collapse" href="javascript:"><span></span></a>
            </div>
            <div class="navbar-content scroll-div">
                <ul class="nav pcoded-inner-navbar" role="tablist" aria-orientation="vertical">
                    <li class="nav-item pcoded-menu-caption">
                        <label>Navigation</label>
                    </li>
                    <li data-username="dashboard" class="nav-item active">
                        <a href="index.html" class="nav-link "><span class="pcoded-micon"><i class="feather icon-home"></i></span><span class="pcoded-mtext">Dashboard</span></a>
                    </li>
                    <!-- Right Classify Menu -->
                    <% if (Array.isArray(htmlWebpackPlugin.options.right_links)) { %>
                    <% for (item of htmlWebpackPlugin.options.right_links) { %>
                        <li data-username="" class="nav-item pcoded-hasmenu">
                            <a href="javascript:" class="nav-link "><span class="pcoded-mtext"><%= item['name'] %></span></a>
                            <ul class="pcoded-submenu">
                                <% for (var i=0; i<item['classify'].length; i++) { %>
                                    <li><a class="" id="tab" data-toggle="pill" href="#<%= item['classify'][i]['id'] %>" role="tab" aria-controls="<%= item['classify'][i]['id'] %>" aria-selected="true"><%= item['classify'][i]['name'] %></a></li>
                                <% } %>
                            </ul>
                        </li>
                    <% } %>
                    <% } %>
                    <li class="nav-item pcoded-menu-caption">
                        <label>Links</label>
                    </li>
                    <!-- Left classify -->
                    <% if (Array.isArray(htmlWebpackPlugin.options.left_links)) { %><%
                        for (item of htmlWebpackPlugin.options.left_links) { %>
                            <li data-username="" class="nav-item pcoded-hasmenu">
                                <a href="javascript:" class="nav-link "><span class="pcoded-mtext"><%= item['name'] %></span></a>
                                <ul class="pcoded-submenu">
                                    <% for (link of item['links']) { %>
                                        <li><a class="" id="home-tab" data-toggle="pill" href="<%= link['link'] %>" role="tab" aria-controls="<%= link['id'] %>" aria-selected="true"><%= link['name'] %></a></li>
                                    <% } %>
                                </ul>
                            </li>
                            <%
                } %><%
            } %><%
            %>
                </ul>
            </div>
        </div>
    </nav>
    <!-- [ navigation menu ] end -->
    <!-- [ Header ] start -->
    <header class="navbar pcoded-header navbar-expand-lg navbar-light">
        <div class="m-header">
            <a class="mobile-menu" id="mobile-collapse1" href="javascript:"><span></span></a>
            <a href="index.html" class="b-brand">
                   <div class="b-bg">
                       <i class="feather icon-trending-up"></i>
                   </div>
                   <span class="b-title">SiteNav</span>
               </a>
        </div>
        <a class="mobile-menu" id="mobile-header" href="javascript:">
            <i class="feather icon-more-horizontal"></i>
        </a>
        <div class="collapse navbar-collapse">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="waves-effect waves-light" href="https://xiaoye.me">Life</a>
                </li>
                <li class="nav-item">
                    <a class="waves-effect waves-light" href="https://dbqf.xyz">Tech</a>
                </li>
                <li class="nav-item">
                    <div class="main-search">
                        <div class="input-group">
                            <input type="text" id="m-search" class="form-control" placeholder="Search . . .">
                            <a href="javascript:" class="input-group-append search-close">
                                <i class="feather icon-x input-group-text"></i>
                            </a>
                            <span class="input-group-append search-btn btn btn-primary">
                                <i class="feather icon-search input-group-text"></i>
                            </span>
                        </div>
                    </div>
                </li>
                
            </ul>
        </div>
    </header>
    <!-- [ Header ] end -->

    <!-- [ Main Content ] start -->
    <div class="pcoded-main-container">
        <div class="pcoded-wrapper">
            <div class="pcoded-content">
                <div class="pcoded-inner-content">
                    <!-- [ breadcrumb ] start -->
                    <!-- [ breadcrumb ] end -->
                    <div class="main-body">
                        <div class="page-wrapper">
                            <!-- [ Main Content ] start -->
                            <div class="col">
                                <!-- Right classify -->
                                <% if (Array.isArray(htmlWebpackPlugin.options.right_links)) { %>
                                <% for (item of htmlWebpackPlugin.options.right_links) { %>
                                <div class="row">
                                    <div class="col">
                                        <div class="card">
                                            <div class="card-header"><h5><%= item['name'] %></h5></div>
                                            <div class="row">
                                                <div class="col-xl-1 col-lg-2 col-md-2 col-sm-12">
                                                    <ul class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                                                        <% for (var i=0; i<item['classify'].length; i++) { %>
                                                        <li><a class="nav-link text-left <%if(i==0){%>active <%}%>" id="v-pills-home-tab" data-toggle="pill" href="#<%= item['classify'][i]['id'] %>" role="tab" aria-controls="<%= item['classify'][i]['id'] %>" aria-selected="true"><%= item['classify'][i]['name'] %></a></li>
                                                        <% } %>
                                                        <!-- <li><a class="nav-link text-left active" id="v-pills-home-tab" data-toggle="pill" href="#v-pills-home" role="tab" aria-controls="v-pills-home" aria-selected="true">Home</a></li> -->
                                                    </ul>
                                                </div>
                                                <div class="col-xl-11 col-lg-10 col-md-10 col-sm-12">
                                                    <div class="tab-content" id="v-pills-tabContent">
                                                        <% for(var i=0; i<item['classify'].length; i++) { %>
                                                            <div class="tab-pane fade <%if(i==0){%>show active <%}%>" id="<%= item['classify'][i]['id'] %>" role="tabpanel" aria-labelledby="<%= item['classify'][i]['id'] %>-tab">
                                                                <div class="row">
                                                                <% for (var link of item['classify'][i]['links']) { %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                                    <div class="col-md-2">
                                                                        <div class="card">
                                                                            <div class="card-body">
                                                                                <h5 class="card-title"><a href="<%= link['link'] %>" target="_blank" title="<%= link['title'] %>">
                                                                                    <b><%= link['name'] %></b>
                                                                                </a></h5>
                                                                                <p class="card-text"><%= link['description'] %></p>
                                                                            </div>
                                                                        
                                                                        </div>
                                                                    </div>
                                                                <% } %>
                                                                </div>
                                                            </div>
                                                        <% } %>
                                                    </div>
                                                </div>
                                            </div>
                                            <% if (Array.isArray(item['special']) && item['special'].length > 0) { %>
                                                <div class="card-footer">
                                                    <% for (special of item['special']) { %>
                                                        <a class="btn btn-light" role="button" href="<%= special['link'] %>" target="_blank" title="<%= special['title'] %>">
                                                            <%= special['name'] %>
                                                        </a>
                                                    <% } %>
                                                </div>
                                            <% } %>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                                <% } %>
                                <!-- End right classify -->
                            </div>
                            <!-- [ Main Content ] end -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- [ Main Content ] end -->

